{
	"name": "nebulosa-server",
	"module": "main.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"lint": "biome lint --write src/",
		"format": "biome format --write src/",
		"prettier": "prettier \"**/*.{vue,css}\" --write",
		"web": "vite",
		"serve": "bun --watch run .",
		"dev": "run-p web serve",
		"compile": "vite build && bun build --compile --minify . --outfile nebulosa.exe"
	},
	"keywords": ["astronomy", "indi"],
	"homepage": "https://tiagohm.github.io/nebulosa",
	"author": {
		"name": "Tiago Melo",
		"email": "tiago.henrique.cco@gmail.com"
	},
	"license": "MIT",
	"dependencies": {
		"@elysiajs/cors": "1.3.1",
		"@elysiajs/cron": "1.3.0",
		"@elysiajs/static": "1.3.0",
		"elysia": "1.3.1",
		"nebulosa": "github:tiagohm/nebulosa.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "latest",
		"@mdi/font": "7.4.47",
		"@primeuix/themes": "1.1.1",
		"@primevue/auto-import-resolver": "4.3.4",
		"@tailwindcss/vite": "4.1.6",
		"@tsconfig/node22": "latest",
		"@types/bun": "latest",
		"@types/node": "latest",
		"@vitejs/plugin-vue": "5.2.4",
		"@vue/tsconfig": "0.7.0",
		"@vueuse/core": "13.2.0",
		"npm-run-all2": "8.0.1",
		"pinia": "3.0.2",
		"prettier": "3.5.3",
		"prettier-plugin-tailwindcss": "0.6.11",
		"primevue": "4.3.4",
		"tailwindcss": "4.1.6",
		"tailwindcss-primeui": "0.6.1",
		"typescript": "5.8.3",
		"unplugin-vue-components": "28.5.0",
		"vite": "6.3.5",
		"vue": "3.5.13"
	},
	"trustedDependencies": ["@biomejs/biome", "esbuild", "nebulosa", "sharp"]
}
