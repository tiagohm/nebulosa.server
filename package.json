{
	"name": "nebulosa-server",
	"module": "main.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"lint": "biome lint --write src/",
		"format": "biome format --write src/",
		"prettier": "prettier \"**/*.{vue,css}\" --write",
		"web": "vite --host",
		"serve": "bun --watch run .",
		"dev": "run-p web serve",
		"compile": "vite build && bun build --compile --minify . --outfile nebulosa.exe",
		"typecheck": "tsc -p tsconfig.serve.json"
	},
	"keywords": ["astronomy", "indi"],
	"homepage": "https://tiagohm.github.io/nebulosa",
	"author": {
		"name": "Tiago Melo",
		"email": "tiago.henrique.cco@gmail.com"
	},
	"license": "MIT",
	"dependencies": {
		"@elysiajs/cors": "1.3.3",
		"@elysiajs/cron": "1.3.0",
		"@elysiajs/eden": "1.3.2",
		"@elysiajs/static": "1.3.0",
		"elysia": "1.3.4",
		"nebulosa": "github:tiagohm/nebulosa.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "latest",
		"@mdi/font": "7.4.47",
		"@primeuix/themes": "1.1.1",
		"@primevue/auto-import-resolver": "4.3.4",
		"@tailwindcss/vite": "4.1.8",
		"@tsconfig/node22": "latest",
		"@types/bun": "latest",
		"@types/node": "latest",
		"@vitejs/plugin-vue": "5.2.4",
		"@vue/tsconfig": "0.7.0",
		"@vueuse/core": "13.3.0",
		"npm-run-all2": "8.0.4",
		"pinia": "3.0.2",
		"prettier": "latest",
		"prettier-plugin-tailwindcss": "latest",
		"primevue": "4.3.4",
		"tailwindcss": "4.1.8",
		"tailwindcss-primeui": "0.6.1",
		"typescript": "latest",
		"unplugin-vue-components": "28.7.0",
		"vite": "6.3.5",
		"vue": "3.5.16"
	},
	"trustedDependencies": ["@biomejs/biome", "@tailwindcss/oxide", "esbuild", "nebulosa", "sharp"]
}
